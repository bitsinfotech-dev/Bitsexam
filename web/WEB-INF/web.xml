    <servlet>
        <servlet-name>Testing</servlet-name>
        <servlet-class>Controller.Testing</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinalUpdate</servlet-name>
        <servlet-class>Controller.FinalUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteSubject</servlet-name>
        <servlet-class>Controller.DeleteSubject</servlet-class>
    </servlet>
    <servlet>
